package com.syntax.class101DataTypes;

public class Alibay1 {

	public static void main(String[] args) {
		
		
		      byte    a=-17;
		      double  b=-5.89;
		      short   v=539;
		      long    d=5679087854l;
		      
		      double  add=a+b;
		      double  sub=d-v;
		      double  mul=a*b;
		      double  div=d/a;
		      
		      System.out.println("additional the is "+a+"  + "+b+" = "+add+" equel Good");
		      System.out.println("subjuction the is "+d+"  - "+v+" = "+sub+" equel Good");
		      System.out.println("multiple the is "+a+"  *  "+b+"  = "+mul+"  equel  mull");
		      System.out.println("division the is "+d+"  /  "+a+"  = wrong (if no want show equel) + div");
		      
		     
		      
		      
	}

}
