package com.syntax01DataTypes;

public class AlibayDebitCard {

	public static void main(String[] args) {
		
		String name=       "Alibay";
		String lastname=   "Aitov";
        String bank=       "TD bank";
        short  account=    2005;
        String card=       "400500600700";
        double expire=     10.23;
        int  SVC=          123;
             
             System.out.println("My name is: " + name);
             System.out.println("My last name is:- " +lastname);
             System.out.println("My bank is: " + bank);
             System.out.println("My bank account is: " + account);
             System.out.println("My card number is: " + card);
             System.out.println("My expire date is: " + expire);
             System.out.println("My SVC is: " + SVC);
        
        
        
	}

}
