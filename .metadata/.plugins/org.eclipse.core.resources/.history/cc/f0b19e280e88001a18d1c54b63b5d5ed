package com.syntax02DataTypesFlDuble;
public class HomeWork2 {
public static void main(String[] args) {
		
		byte        a=120;       a=127;
		short       b=2500;      b=32767;
		int         c=390000;    c=387000;
		long        d=100l;      d=150l;
		float       e=15.50f;    e=18.90f;
		double      f=1690.987;  f=1700.90;

		double f1=16.99; //if we write double before, but second just type "f". 
		char dinara='F';      
		boolean happy=true;
		
		dinara='M';
		happy=false;
		
		System.out.println(happy);
		//happy=false;
		//happy=123; Only use: True or False on the boolean.
		System.out.println(happy);
		f1= 16.99;// this is double.
		//char f='F'; f error will show duplicate variable.
}}
